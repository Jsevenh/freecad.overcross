<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>freecad_cross_rosdep</name>
  <version>1.0.0</version>
  <description>Package to store freecad.cross workbench dependencies</description>
  <maintainer email="it.project.devel@gmail.com">Vasily S</maintainer>
  <license>LGPL-2.1</license>

  <depend>geometry_msgs</depend>
  <depend>moveit_msgs</depend>
  <depend>shape_msgs</depend>
  <depend>urdfdom_py</depend>
  <depend>xacro</depend>
  <depend>python3-requests</depend>
  <depend>python3-collada</depend>
  <depend>ament_index_python</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
